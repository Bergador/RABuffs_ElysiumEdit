	<Frame name="RABFrame" toplevel="true" movable="true" enableMouse="true" parent="UIParent" frameStrata="LOW" clampedToScreen="true">
		<Size>
			<AbsDimension x="130" y="150" />
		</Size>
		<Anchors>
			<Anchor point="CENTER" />
		</Anchors>
		<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
			<BackgroundInsets>
				<AbsInset left="5" right="5" top="5" bottom="5" />
			</BackgroundInsets>
			<TileSize>
				<AbsValue val="16" />
			</TileSize>
			<EdgeSize>
				<AbsValue val="16" />
			</EdgeSize>
		</Backdrop>

		<Frames>
			<Button name="RAB_Title">
				<Size>
					<AbsDimension x="115" y="22" />
				</Size>
				<Anchors>
					<Anchor point="BOTTOM" relativeTo="RABFrame" relativePoint="TOP">
						<Offset>
							<AbsDimension x="0" y="-5" />
						</Offset>
					</Anchor>
				</Anchors>
				<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
					<BackgroundInsets>
						<AbsInset left="3" right="4" top="5" bottom="5" />
					</BackgroundInsets>
					<TileSize>
						<AbsValue val="16" />
					</TileSize>
					<EdgeSize>
						<AbsValue val="16" />
					</EdgeSize>
				</Backdrop>
				<Scripts>
					<OnMouseUp>
						if ( RABFrame.isMoving ) then
							RABFrame:StopMovingOrSizing();
							RABFrame.isMoving = false;
						end
					</OnMouseUp>
					<OnMouseDown>
						if (arg1 == "LeftButton" and RABui_Settings.lockwindow ~= true) then
							RABFrame:StartMoving();
							RABFrame.isMoving = true;
						elseif (arg1 == "RightButton") then
							ToggleDropDownMenu(1, nil, RAB_Menu, "cursor",-10,8);
						end
					</OnMouseDown>
					<OnHide>
						if ( RABFrame.isMoving ) then
							RABFrame:StopMovingOrSizing();
							RABFrame.isMoving = false;
						end
					</OnHide>
				</Scripts>
				<NormalText inherits="GameFontNormalSmall" justifyH="CENTER" />
			</Button>
			<Frame name="RAB_Menu" inherits="UIDropDownMenuTemplate" hidden="true">
				<Scripts>
					<OnLoad>	RABui_Menu_OnLoad();	</OnLoad>
				</Scripts>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad>
				RABui_OnLoad();     
			</OnLoad>
			<OnUpdate>
				if (RABui_OnUpdate ~= nil) then
					RABui_OnUpdate(arg1);
				end
			</OnUpdate>
			<OnEvent>
				if (RABui_OnEvent ~= nil) then
					RABui_OnEvent();
				end
			</OnEvent>
		</Scripts>
	</Frame>


