	<CheckButton name="RAB_Settings_CheckBox" virtual="true" inherits="OptionsCheckButtonTemplate">
		<Size>
			<AbsDimension x="25" y="25"/>
		</Size>
		<Scripts>
			<OnClick> RABui_Settings_ToggleOption(this.name); </OnClick>
			<OnShow> RABui_Settings_InitOption(); </OnShow>
			<OnEnter>GameTooltip:SetOwner(this, "ANCHOR_RIGHT");
				 GameTooltip:AddLine(getglobal(this:GetName() .. "Text"):GetText(),1,1,1);
				 GameTooltip:AddLine(this.tooltipText,nil,nil,nil,1); GameTooltip:Show();</OnEnter>
			<OnLeave> GameTooltip:Hide(); </OnLeave>
		</Scripts>
	</CheckButton>
	<Button name="RAB_SettingsUTabButton" virtual="true" inherits="TabButtonTemplate">
		<Scripts>
			<OnClick> RABui_Settings_SelectUTab(this:GetID()); </OnClick>
			<OnLoad> PanelTemplates_TabResize(0); _,_, t, n = string.find(this:GetName(),"(.+)(%d+)$"); n = tonumber(n); if (n > 1) then this:SetPoint("TOPLEFT",t .. (n-1),"TOPRIGHT"); end</OnLoad>
		</Scripts>
	</Button>
	<Button name="RAB_SettingsTabButton" virtual="true" inherits="CharacterFrameTabButtonTemplate">
		<Scripts>
			<OnClick> RABui_Settings_SelectTab(this:GetID()); PanelTemplates_Tab_OnClick(this:GetParent()); </OnClick>
			<OnLoad> PanelTemplates_TabResize(0); _,_, t, n = string.find(this:GetName(),"(.+)(%d+)$"); n = tonumber(n); if (n > 1) then this:SetPoint("TOPLEFT",t .. (n-1),"TOPRIGHT",-16,0); end</OnLoad>
		</Scripts>
	</Button>
